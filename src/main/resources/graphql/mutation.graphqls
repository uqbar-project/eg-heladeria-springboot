type Mutation {
  actualizarHeladeria(heladeria: ActualizarHeladeriaInput!): Heladeria!
  #Actualizar uno o más atributos de una heladeria
  crearDuenio(duenio: DuenioInput!): Duenio!
  #Crear un nuevo dueño
  agregarGustos(heladeriaId: ID!, gustos: [AgregarGustoInput!]!): Heladeria!
  #Agregar uno o más gustos a una heladería
  eliminarGustos(heladeriaId: ID!, gustos: [EliminarGustoInput!]!): Heladeria!
  #Eliminar uno o más gustos de una heladería
}
