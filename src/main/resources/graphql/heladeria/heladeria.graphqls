type Heladeria {
  id: ID!
  nombre: String!
  duenio: Duenio
  heladeria: Heladeria
  tipoHeladeria: TipoHeladeria!
  gustos: [Gusto]
}

enum TipoHeladeria {
  ECONOMICA
  ARTESANAL
  INDUSTRIAL
}

input ActualizarHeladeriaInput {
  id: ID!
  nombre: String
  tipoHeladeria: TipoHeladeria
  duenio: DuenioInput
}
